{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
    "types": [
      "@types/jest",
      "nativewind/types",
      "webpack-env"
    ]
  },
  "include": [
    "expo-plugins",
    ".expo",
    "src",
    "*.ts",
    "**/*.js",
    "index.ts"
  ],
  "exclude": [
    "*.test.ts"
  ]
}