{
  "cli": {
    "version": ">= 5.9.1",
    "requireCommit": false,
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.9.0",
      "bun": "1.1.26",
      "env": {
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "***REMOVED***",
        "EXPO_PUBLIC_CF_IMAGES_ACC_HASH": "***REMOVED***",
        "EXPO_PUBLIC_UBER_CLIENT_ID": "***REMOVED***"
      },
      "channel": "base"
    },
    "development": {
      "extends": "base",
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "development"
      }
    },
    "development-simulator": {
      "extends": "development",
      "channel": "development-simulator",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "local"
      }
    },
    "preview": {
      "extends": "base",
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "preview"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "developmentClient": false,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      },
      "ios": {}
    },
    "internal": {
      "android": {
        "track": "internal"
      },
      "ios": {}
    }
  }
}
